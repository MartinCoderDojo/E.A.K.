{
  "name": "web-platform",
  "version": "0.0.0",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "underscore": "latest",
    "jquery": "latest",
    "backbone": "latest",
    "modernizr": "latest",
    "codemirror": "4.2.x",
    "slowparse": "~0.0.1",
    "stats.js": "*",
    "prelude-ls": "~1.1.1",
    "async": "~0.9.0",
    "popcornjs": "~1.5.6"
  },
  "overrides": {
    "underscore": {
      "main": "underscore.js"
    },
    "backbone": {
      "main": "backbone.js",
      "dependencies": {
        "underscore": "*",
        "jquery": "*"
      }
    },
    "modernizr": {
      "main": [
        "modernizr.js",
        "feature-detects/css-calc.js",
        "feature-detects/css-boxsizing.js"
      ]
    },
    "prelude-ls": {
      "main": "browser/prelude-browser.js"
    },
    "codemirror": {
      "main": [
        "lib/codemirror.js",
        "mode/xml/xml.js",
        "mode/css/css.js",
        "mode/htmlmixed/htmlmixed.js"
      ]
    },
    "slowparse": {
      "main": [
        "slowparse.js",
        "tree-inspectors.js",
        "spec/errors.jquery.js"
      ]
    },
    "stats.js": {
      "main": "build/stats.min.js"
    },
    "async": {
      "main": "lib/async.js"
    },
    "popcornjs": {
      "main": ["popcorn.js", "plugins/subtitle/popcorn.subtitle.js"]
    }
  }
}
